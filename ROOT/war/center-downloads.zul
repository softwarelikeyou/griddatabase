    <group label="Ancillary Services Daily Reports" />
    <row>
     <cell>
      <label sclass="label" value="Start Date"/>
      <space spacing="3"/>
      <datebox id="startDateboxASCPC" 
	           format="MM/dd/yyyy kk:mm:ss"
	           timeZone="GMT-6" 
	           timeZonesReadonly="false"
	           constraint="after 20031231"
	           cols="20" 
	           mold="rounded" 
	           style="white-space: normal;">
	   <attribute name="onCreate">
          java.util.Calendar calendar = java.util.Calendar.getInstance();
          calendar.set(java.util.Calendar.YEAR, 2004);
          calendar.set(java.util.Calendar.MONTH, 0);
          calendar.set(java.util.Calendar.DAY_OF_MONTH, 1);
          calendar.set(java.util.Calendar.HOUR, 0);
          calendar.set(java.util.Calendar.MINUTE, 0);
          calendar.set(java.util.Calendar.SECOND, 0);
          startDateboxASCPC.setValue(calendar.getTime());
       </attribute>
      </datebox>
	  <space spacing="3"/>
      <label sclass="label" value="End Date"/>
      <space spacing="3"/>
      <datebox id="endDateboxASCPC" 
	           format="MM/dd/yyyy kk:mm:ss"
	           onCreate="self.value = new Date(System.currentTimeMillis())"
	           timeZone="GMT-6" 
	           timeZonesReadonly="false"
	           constraint="no empty, no future: now or never" 
	           cols="20" 
	           mold="rounded" 
	           style="white-space: normal;"/>
     </cell>
     <button id="downloadASCPC" label="Download"/>
	</row>
         
