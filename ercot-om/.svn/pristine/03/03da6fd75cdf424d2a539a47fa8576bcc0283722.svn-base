package com.softwarelikeyou.exception;

import java.io.Serializable;

public class UserException extends LocalizedException { 
	private static final long	serialVersionUID	= 1L;

	public UserException() {
		super();
	}
	
	public UserException(String msg) {
		super(msg);
	}
	
	public UserException(String msgKey, Serializable... varargs) {
		super(msgKey, varargs);
	}
	
	public UserException(Throwable t, String msgKey) {
		super(t, msgKey);
	}

	public UserException(Throwable t, String msgKey, Serializable... varargs) {
		super(t, msgKey, varargs);
	}	
	
	public UserException(Throwable t) {
		super(t);
	}		
	
}
