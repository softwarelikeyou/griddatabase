package com.softwarelikeyou.exception;

import java.io.Serializable;

public class SchedulerException extends LocalizedException { 
	private static final long	serialVersionUID	= 1L;

	public SchedulerException() {
		super();
	}
	
	public SchedulerException(String msg) {
		super(msg);
	}
	
	public SchedulerException(String msgKey, Serializable... varargs) {
		super(msgKey, varargs);
	}
	
	public SchedulerException(Throwable t, String msgKey) {
		super(t, msgKey);
	}

	public SchedulerException(Throwable t, String msgKey, Serializable... varargs) {
		super(t, msgKey, varargs);
	}	
	
	public SchedulerException(Throwable t) {
		super(t);
	}		
	
}
