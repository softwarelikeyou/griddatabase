package com.softwarelikeyou.exception;

import java.io.Serializable;

public class CountyContourException extends LocalizedException { 
	private static final long	serialVersionUID	= 1L;

	public CountyContourException() {
		super();
	}
	
	public CountyContourException(String msg) {
		super(msg);
	}
	
	public CountyContourException(String msgKey, Serializable... varargs) {
		super(msgKey, varargs);
	}
	
	public CountyContourException(Throwable t, String msgKey) {
		super(t, msgKey);
	}

	public CountyContourException(Throwable t, String msgKey, Serializable... varargs) {
		super(t, msgKey, varargs);
	}	
	
	public CountyContourException(Throwable t) {
		super(t);
	}		
	
}
